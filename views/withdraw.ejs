 var hour ="0"+ today.getHours();
 var minute ="0"+ today.getMinutes();
 var second ="0"+ today.getSeconds();
 var prepand = (hour >= 12)? " PM ":" AM ";
 hour = (hour >= 12)? hour - 12: hour;
 if (hour===0 && prepand===' PM ') 
 { 
 if (minute===0 && second===0)
 { 
 hour=12;
 prepand=' Noon';
 } 
 else
 { 
 hour=12;
 prepand=' PM';
 } 
 } 
 if (hour===0 && prepand===' AM ') 
 { 
 if (minute===0 && second===0)
 { 
 hour=12;
 prepand=' Midnight';
 } 
 else
 { 
 hour=12;
 prepand=' AM';
 } 
 } 

function dateOrdinal(day) {
   return day+(31==day||21==day||1==day?"st":22==day||2==day?"nd":23==day||3==day?"rd":"th")
};



document.getElementById("flipper").innerHTML = daylist[day] +' '+ dateOrdinal(flu) +' '+ monthlist[n] +' '+ hour +':'+ minute.substr(-2) +':'+ second.substr(-2) + ' ('+prepand+')';
 }
, 1000);



